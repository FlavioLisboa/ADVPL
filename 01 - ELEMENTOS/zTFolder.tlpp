#Include 'Totvs.ch'
#Include 'Protheus.ch'


User Function zTFolder()

Local oDlg1    
Local oFolder
Local aFolder

Local oGet1
Local oGet2
Local oGet3


Local cGet1
Local cGet2
Local cGet3


Local oSay1
Local oSay2
Local oSay3

oDlg1    := TDialog():New(0,0,300,400, 'Painel de Abas',,,,,CLR_BLACK,CLR_WHITE,,,.T.)

aFolder  := {"Cliente","Fornecedor","Produto"}
