oButton2 := TButton():New(25,10,"Executar",oFolder:aDialogs[2],{|| MsgInfo(cGet2,"Conteúdo Digitado")}, 60,15,,,.F.,.T.,.F.,,.F.,,,.F.)

oSay3    := TSay():New(10,10,{||"Produto"},oFolder:aDialogs[3],,,,,,.T.,CLR_BLUE,CLR_WHITE,300,20)
cGet3    := Space(30)
oGet3    := TGet():New(10,01,{|u| IIF(Pcount()>0,cGet2:=u,cGet2)},oFolder:aDialogs[3],096,009,"",,0,,,.F.,,.T.,,.F.,,.F.,.F.,,.F.,.F.,,cGet3,,,, )
oButton3 := TButton():New(25,10,"Executar",oFolder:aDialogs[3],{|| MsgInfo(cGet3,"Conteúdo Digitado")}, 60,15,,,.F.,.T.,.F.,,.F.,,,.F. )


oDlg1:Activate(,,,.T.)


Return 
