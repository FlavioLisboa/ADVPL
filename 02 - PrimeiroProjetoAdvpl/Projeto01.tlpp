//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
//%%%%                                          Função Para Mostrar os CheckBox Marcados  (CheckOption)                                                     %%%%%%
//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

Static Function CheckOptions()

if lCheckAdvpl .AND. (lCheckCSHARP .OR. lCheckPython .OR. lCheckSQL) = .F.
    MsgInfo("Somente o Advpl está marcada")
elseif lCheckCSHARP .AND. (lCheckAdvpl .OR. lCheckPython .OR. lCheckSQL) = .F.
    MsgInfo("Somente o CSHARP está marcada")
elseif lCheckSQL .AND. (lCheckAdvpl .OR. lCheckPython .OR. lCheckCSHARP) = .F.
    MsgInfo("Somente SQL está marcada")
elseif lCheckPython .AND. (lCheckAdvpl .OR. lCheckCSHARP .OR. lCheckSQL) = .F.
    MsgInfo("Somente Python está marcada")
elseif lCheckAdvpl .AND. lCheckCSHARP .AND. (lCheckPython .OR. lCheckSQL) =.F.
    MsgInfo("As opções ADVPL e CSHARP estão marcadas")
elseif lCheckAdvpl .AND. lCheckSQL .AND. (lCheckPython .OR. lCheckCSHARP) = .F.
    MsgInfo("As opções ADVPL e SQL estão marcadas")
elseif lCheckAdvpl .AND. lCheckPython .AND. (lCheckCSHARP .OR. lCheckSQL) = .F.
    MsgInfo("As opções ADVPL e Python estão marcadas")
elseif lCheckCSHARP .AND. lCheckSQL .AND. (lCheckPython .OR. lCheckAdvpl) = .F.
    MsgInfo("As opções CSHARP e SQL estão marcadas")   
elseif lCheckCSHARP .AND. lCheckPython .AND. (lCheckSQL .OR. lCheckAdvpl) =.F.
    MsgInfo("As opções CSHARP e Python estão marcadas")
elseif lCheckPython .AND. lCheckSQL .AND. (lCheckAdvpl .OR. lCheckCSHARP) = .F.
    MsgInfo("As opções Python e SQL estão marcadas")
elseif lCheckPython .AND. lCheckAdvpl .AND. lCheckCSHARP .AND. !lCheckSQL
    MsgInfo("As opções ADVPL, CSHARP e Python estão marcadas")
elseif !lCheckPython .AND. lCheckAdvpl .AND. lCheckCSHARP .AND. lCheckSQL
    MsgInfo("As opções ADVPL, CSHARP e SQL estão marcadas")
elseif lCheckPython .AND. !lCheckAdvpl .AND. lCheckCSHARP .AND. lCheckSQL
    MsgInfo("As opções CSHARP, Python e SQL estão marcadas")    
elseif lCheckPython .AND. lCheckAdvpl .AND. !lCheckCSHARP .AND. lCheckSQL
    MsgInfo("As opções ADVPL, Python e SQL estão marcadas")  
elseif lCheckPython .AND. lCheckAdvpl .AND. lCheckCSHARP .AND. lCheckSQL 
    MsgInfo("Todas as opções estão marcadas")
else
    MsgInfo("Todas as opções estão desmarcadas")
endif
    
Return 

//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
//%%%%                                          Função Para Marcar Todos CheckBox (MarcarTodos)                                                             %%%%%%
//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%


Static Function MarcarTodos()

lCheckAdvpl  := .T.
lCheckCSHARP := .T. 
lCheckSQL    := .T.
lCheckPython := .T.

oCheckAdvpl:CtrlRefresh()
oCheckCSHARP:CtrlRefresh()
oCheckSQL:CtrlRefresh()
oCheckPython:CtrlRefresh()
    
Return 

//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
//%%%%                                          Função Para Desmarcar Todos CheckBox   (Desmarcar)                                                          %%%%%%
//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

Static Function Desmarcar()

lCheckAdvpl  := .F.
lCheckCSHARP := .F.
lCheckSQL    := .F.
lCheckPython := .F.

oCheckAdvpl:Refresh()
oCheckCSHARP:Refresh()
oCheckSQL:Refresh()
oCheckPython:Refresh()
    
Return 

